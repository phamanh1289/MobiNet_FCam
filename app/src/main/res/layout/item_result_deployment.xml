<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_horizontal"
    android:orientation="vertical"
    tools:ignore="MissingPrefix">

    <TextView
        android:id="@+id/textView"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/item_result_contract"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/itemResultDeployment_tvContract"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/itemResultDeployment_tvContract"/>

    <TextView
        android:id="@+id/textView2"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/item_result_create_date"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/itemResultDeployment_tvCreateDate"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/itemResultDeployment_tvCreateDate"/>

    <TextView
        android:id="@+id/textView3"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/item_result_assign_date"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/itemResultDeployment_tvAssignDate"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/itemResultDeployment_tvAssignDate"/>

    <TextView
        android:id="@+id/textView4"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8dp"
        android:text="@string/item_result_service_type"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/itemResultDeployment_tvServiceType"/>

    <TextView
        android:id="@+id/textView5"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/item_result_priority"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintBottom_toBottomOf="@+id/itemResultDeployment_tvPriority"
        app:layout_constraintEnd_toEndOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="@+id/textView4"
        app:layout_constraintTop_toTopOf="@+id/itemResultDeployment_tvPriority"/>

    <TextView
        android:id="@+id/itemResultDeployment_tvContract"
        fontPath="@string/Helvetica_Bold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_8dp"
        android:layout_marginTop="@dimen/_8dp"
        android:layout_marginEnd="@dimen/_8dp"
        android:textColor="@color/redMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/itemResultDeployment_tvCreateDate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintEnd_toEndOf="@+id/itemResultDeployment_tvContract"
        app:layout_constraintStart_toStartOf="@+id/itemResultDeployment_tvContract"
        app:layout_constraintTop_toBottomOf="@+id/itemResultDeployment_tvContract"/>

    <TextView
        android:id="@+id/itemResultDeployment_tvAssignDate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintEnd_toEndOf="@+id/itemResultDeployment_tvCreateDate"
        app:layout_constraintStart_toStartOf="@+id/itemResultDeployment_tvCreateDate"
        app:layout_constraintTop_toBottomOf="@+id/itemResultDeployment_tvCreateDate"/>

    <TextView
        android:id="@+id/itemResultDeployment_tvServiceType"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:lineSpacingExtra="@dimen/_4dp"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintEnd_toEndOf="@+id/itemResultDeployment_tvAssignDate"
        app:layout_constraintStart_toStartOf="@+id/itemResultDeployment_tvAssignDate"
        app:layout_constraintTop_toBottomOf="@+id/itemResultDeployment_tvAssignDate"/>

    <TextView
        android:id="@+id/itemResultDeployment_tvPriority"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_8dp"
        android:textColor="@color/blueMain"
        android:textSize="@dimen/_17dp"
        app:layout_constraintEnd_toEndOf="@+id/itemResultDeployment_tvServiceType"
        app:layout_constraintStart_toStartOf="@+id/itemResultDeployment_tvServiceType"
        app:layout_constraintTop_toBottomOf="@+id/itemResultDeployment_tvServiceType"/>

    <View
        android:id="@+id/view"
        android:layout_width="0dp"
        android:layout_height="@dimen/_8dp"
        android:layout_marginTop="@dimen/_8dp"
        android:background="@color/silver"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/itemResultDeployment_tvPriority"/>
</android.support.constraint.ConstraintLayout>
